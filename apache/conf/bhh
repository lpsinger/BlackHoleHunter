<VirtualHost *:80>
	ServerAdmin webmaster@localhost
	
	DocumentRoot "C:/xampplite/htdocs/BlackHoleHunter/public_html"
	<Directory />
		Options FollowSymLinks
		AllowOverride None
	</Directory>

<IfModule alias_module>
    ScriptAlias /cgi-bin/ "C:/xampplite/htdocs/BlackHoleHunter/public_html/cgi-bin/"
</IfModule>

LoadModule access_module modules/mod_access.so

<Directory "C:/xampplite/htdocs/BlackHoleHunter/public_html/cgi-bin/">
    AllowOverride None
    Options -MultiViews +SymLinksIfOwnerMatch +ExecCGI
    Order allow,deny
    Allow from all
    AddHandler cgi-script .pl
    AddHandler cgi-script .py
</Directory>

#	LogLevel debug
#	ErrorLog /var/log/apache2/blackholehunter-error.log
#	CustomLog /var/log/apache2/blackholehunter-access.log combined
</VirtualHost>
